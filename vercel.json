{
  "version": 2,
  "builds": [
    {
      "src": "dist/index.js",
      "use": "@vercel/node",
      "config": {
        "includeFiles": ["./dist/*"]
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "./dist/index.js"
    }
  ],
  "env": {
    "MYSQL_URI": "@mysql_url",
    "MYSQL_USER": "@mysql_user",
    "MYSQL_PASSWORD": "@mysql_password",
    "MYSQL_DATABASE": "@mysql_database"
  }
}
